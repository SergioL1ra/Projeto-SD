li $t0, 70            # Carrega o valor 70 no registrador $t0
sw $t0, 8($zero)      # Armazena o valor de $t0 (70) na posição de memória 8
li $t1, 60            # Carrega o valor 60 no registrador $t1
sw $t1, 9($zero)      # Armazena o valor de $t1 (60) na posição de memória 9
sub $t2, $t0, $t1     # $t2 recebe a subtração: $t0 - $t1 = 70 - 60 = 10
sw $t2, 10($zero)     # Armazena o valor de $t2 (10) na posição de memória 10
